{
  "name": "damned",
  "version": "1.0.0",
  "description": "An ungodly resurrection of chjj's blessed.",
  "scripts": {
    "start": "cross-env TS_NODE_COMPILER_OPTIONS={\\\"target\\\":\\\"ES2015\\\"} ts-node src/index.ts",
    "build": "rollup --config --watch",
    "csscomb": "csscomb --config docs/css/.csscomb.json docs/css/style.scss docs/css/partials/*.scss",
    "sassc": "sass --watch docs/css/style.scss:docs/css/style.min.css --no-cache --sourcemap=none --style=compressed",
    "serve": "npx http-server docs/ -c-1 -o",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brianjenkins94/damned.git"
  },
  "author": "Brian Jenkins",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brianjenkins94/damned/issues"
  },
  "homepage": "https://github.com/brianjenkins94/damned#readme",
  "devDependencies": {
    "@types/ansi-escapes": "^3.0.1",
    "@types/node": "^11.11.3",
    "rollup": "^1.6.0",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-typescript2": "^0.20.1",
    "ts-node": "^8.0.3",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "ansi-escapes": "^3.2.0",
    "cross-env": "^5.2.0",
    "lodash.merge": "^4.6.1",
    "xterm": "^3.12.0"
  }
